{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "underscore": "./node_modules/backbone.marionette/node_modules/underscore/underscore.js",
    "plugin": "./src/javascript/vendor/jquery-plugin.js",
    "backbone": "./node_modules/backbone.marionette/node_modules/backbone/backbone.js",
    "marionette": "./node_moduls/backbone.marionette/lib/backbone.marionette.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$"
      ]
    },
    "marionette": {
      "exports": "Marionette",
      "depends": [
        "underscore:_",
        "jquery:$",
        "backbone:Backbone"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "^1.0.0",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-coffeelint": "^0.4.0",
    "gulp-imagemin": "^0.6.2",
    "gulp-jade": "^0.10.0",
    "gulp-jst-concat": "0.0.1",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-sass": "^1.1.0",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone.marionette": "^2.2.2",
    "gulp-sass": "^1.1.0",
    "jquery": "~2.1.0"
  }
}
